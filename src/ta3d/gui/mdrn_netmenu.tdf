[window]
{
    name=netmenu;
	title=Net Menu;
	uformat=UTF8;
	x=0;
	y=0;
	width=1280;
	height=1024;
	draw borders=false;
	show title=false;
	number of objects=10;
	hidden=false;
	lock=true;
	color=0x00000000;			// Fully transparent so we can see the AREA background
	background window=true;
	fullscreen=true;			// Scale the window to fit the screen
	screen width=1280;
	screen height=1024;
	[object0]		// Log in
	{
		name=b_login;
		type=BUTTON;
		caption=Log in;
		x1=1039;
		y1=960;
		x2=1190;
		y2=997;
		size=2;
	}
	[object1]		// Back
	{
		name=b_back;
		type=BUTTON;
		caption=Back;
		x1=839;
		y1=960;
		x2=990;
		y2=997;
		size=2;
	}
	[object2]		// peer list
	{
		name=peer_list;
		type=LIST;
		x1=1040;
		y1=154;
		x2=1242;
